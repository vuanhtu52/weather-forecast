{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "quotes": ["error", "double"],
        "indent": ["error", 4],
        "no-plusplus": "off",
        "no-underscore-dangle":  ["error", { "allow": true }] 
    }
}
